{
  "name": "marsha",
  "version": "4.2.1",
  "description": "üê† a FUN LTI video provider",
  "main": "front/index.tsx",
  "scripts": {
    "build": "tsc --noEmit && webpack",
    "build-dev": "yarn copy-iframe-resizer; yarn copy-katex-fonts; yarn build",
    "copy-iframe-resizer": "cp ../../node_modules/iframe-resizer/js/*.min.js ../../../backend/marsha/static/js",
    "copy-katex-fonts": "mkdir -p ../../../backend/marsha/static/css/fonts && cp ../../node_modules/katex/dist/fonts/* ../../../backend/marsha/static/css/fonts",
    "lint": "eslint **/*.{ts,tsx}",
    "lint:fix": "yarn run lint --fix",
    "prettier": "prettier --list-different '**/*.+(ts|tsx|json|js|jsx)' --ignore-path ../../../.prettierignore",
    "prettier-write": "prettier --write '**/*.+(ts|tsx|json|js|jsx)' --ignore-path ../../../.prettierignore",
    "build-sass": "sass scss/_main.scss ../../../backend/marsha/static/css/main.css --load-path=../../node_modules",
    "build-sass-production": "sass scss/_main.scss ../../../backend/marsha/static/css/main.css --style=compressed --load-path=../../node_modules",
    "test": "cross-env TZ=UTC jest",
    "test-summary": "cross-env TZ=UTC jest 2>&1 | grep 'FAIL' | sort -t: -u -k1,1",
    "test-coverage": "sh -c ' cross-env TZ=UTC jest $0 --coverage --collectCoverageFrom=\"$0/**\"'",
    "test-screenshots": "cross-env TZ=UTC jest --testNamePattern='\\[screenshot\\]'"
  },
  "repository": "git@github.com:openfun/marsha.git",
  "author": "France Universit√© Num√©rique",
  "license": "MIT",
