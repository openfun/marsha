    "@testing-library/react": "9.3.0",
    "@types/fetch-mock": "7.3.1",
    "@types/iframe-resizer": "3.5.7",
    "@types/jest": "24.0.18",
    "@types/jwt-decode": "2.2.1",
    "@types/luxon": "1.15.2",
    "@types/rc-tooltip": "3.7.1",
