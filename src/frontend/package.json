    "@babel/preset-typescript": "7.12.1",
    "@formatjs/cli": "2.13.5",
    "@testing-library/jest-dom": "5.11.5",
    "@testing-library/react": "11.1.0",
    "@types/fetch-mock": "7.3.3",
    "@types/hls.js": "0.13.2",
    "@types/iframe-resizer": "3.5.8",
    "@types/jest": "26.0.15",
