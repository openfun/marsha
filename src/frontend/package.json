{
  "name": "marsha",
  "version": "3.11.0",
  "description": "üê† a FUN LTI video provider",
  "main": "front/index.tsx",
  "scripts": {
    "build": "tsc --noEmit && webpack",
    "build-dev": "yarn copy-iframe-resizer; yarn build",
    "extract-translations": "formatjs extract './**/*.ts*' --ignore ./node_modules --ignore './**/*.d.ts' --out-file i18n/frontend.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format crowdin",
    "compile-translations": "formatjs compile-folder --format crowdin ./i18n ./translations",
    "copy-iframe-resizer": "cp node_modules/iframe-resizer/js/iframeResizer.min.js ../backend/marsha/static/js",
    "lint": "tslint -c tslint.json '**/*.ts?(x)' -e 'node_modules/**/*'",
    "lint-fix": "tslint --fix -c tslint.json '**/*.ts?(x)' -e 'node_modules/**/*'",
    "prettier": "prettier --list-different '**/*.+(ts|tsx|json|js|jsx)' --ignore-path ../.prettierignore",
    "prettier-write": "prettier --write '**/*.+(ts|tsx|json|js|jsx)' --ignore-path ../.prettierignore",
    "lint-all": "yarn lint-fix && yarn prettier-write",
    "sass": "node-sass --include-path node_modules scss/_main.scss ../backend/marsha/static/css/main.css",
    "sass-production": "node-sass --include-path node_modules scss/_main.scss --output-style compressed ../backend/marsha/static/css/main.css",
    "test": "jest"
  },
  "repository": "git@github.com:openfun/marsha.git",
  "author": "France Universit√© Num√©rique",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.12.3",
