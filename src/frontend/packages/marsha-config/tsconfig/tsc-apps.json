{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Marsha Apps",
  "extends": "./tsc-base.json",
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "incremental": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "module": "esnext",
    "noFallthroughCasesInSwitch": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "es5"
  },
  "exclude": ["node_modules"]
}
