{
  "extends": "marsha-config/tsconfig/tsc-packages.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@lib-classroom/*": ["./*"],
      "@lib-common/*": ["../../lib_common/src/*"],
      "@lib-components/*": ["../../lib_components/src/*"],
      "@lib-tests/*": ["../../lib_tests/src/*"],
      "converse": ["../../lib_components/src/types/libs/converse"],
      "JitsiMeetExternalAPI": [
        "../../lib_components/src/types/libs/JitsiMeetExternalAPI"
      ],
      "react-icalendar-link": [
        "../../lib_components/src/types/libs/react-icalendar-link"
      ]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.d.ts"],
  "exclude": ["lib", "node_modules"]
}
